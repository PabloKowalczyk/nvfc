{
    "name": "pablok/nvfc",
    "description": "Fan speed controller for Nvidia graphic cards",
    "keywords": [
        "reactphp",
        "nvidia-gpu-fan-control",
        "nvidia-gpu-fan-speed",
        "fan-controller"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-pcntl": "*",
        "react/child-process": "^0.6.1",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "react/stream": "^1.1",
        "symfony/console": "^4.2",
        "symfony/process": "^4.2"
    },
    "require-dev": {
        "localheinz/composer-normalize": "^1.2",
        "phpunit/phpunit": "^8.2",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "NvFanController\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NvFanController\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/nvfc"
    ]
}
